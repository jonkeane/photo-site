+									        sexif += 'Captured at&nbsp; ' + item.exif.time;
+									      }
+									      }
+												content += '<div class="nGY2PopupOneItemText">' + sexif + '</div>';
+
+												if( item.tags.length > 0 ) {
+													tags = ''
+													for(var i = 0; i < item.tags.length; i++) {
