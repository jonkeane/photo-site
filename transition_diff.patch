+        if( h != 'auto' ) {
+          // grid or justified layout
+          G.tn.defaultSize.height.l1[lst[i]] = h;  //+G.tn.labelHeight.get();
+        }
+        else {
+          var w = G.tn.settings.width.l1[lst[i]];
