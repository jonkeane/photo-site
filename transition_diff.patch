+/* wide button */
+.fullwidth {
+	margin: 2rem auto;
+	width: 100%;
+	font-weight: 900;
+    font-size: 2.25rem;
+    padding: 1rem;
+    line-height: 1;
+    height: auto;
+    white-space: normal;
+}
+
+/* nano gallery overrides */
+
+#nanogallery2 {
+    margin-bottom: 1rem;
+}
+
+.nGY2 .nGY2GThumbnailAlbumTitle, .nGY2 .nGY2GThumbnailImageTitle {
+    text-shadow: none;
+}
+
+.nGY2PopupContent,
+.nGY2PopupContentCenter,
+.nGY2PopupContentLeft,
+.nGY2PopupContentRight {
+	max-height: 90%;
+	overflow-y: auto;
+}
+
+.nGY2PopupCloseButton {
+	position: sticky;
+	top: 0px;
+	background-color: rgba(255, 255, 255, 0.75);
+	width: fit-content;
+	float: right;
+	padding-left: 4px;
+	padding-right: 4px;
+}
+
+/* floating gallery back button */
+/* TODO: get rid of this when the menu is appeared */
+#galleryBackBtn {
+	position: fixed;
